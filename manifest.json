{
    "manifest_version": 3,
    "name": "Webpage Screenshot",
    "version": "1.0",
    "description": "Capture and download screenshot of the visible webpage",
    "permissions": [
        "activeTab",
        "downloads",
        "tabs",
        "desktopCapture"
    ],
    "action": {
        "default_popup": "popup.html"
      }
  }
  